    <action>
      <name>X_SetZoom</name>
      <argumentList>
        <argument>
          <name>InstanceID</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_InstanceID</relatedStateVariable>
        </argument>
        <argument>
          <name>x</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Coordinate</relatedStateVariable>

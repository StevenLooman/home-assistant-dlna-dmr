          <relatedStateVariable>A_ARG_TYPE_InstanceID</relatedStateVariable>
        </argument>
        <argument>
          <name>VideoPID</name>
          <direction>out</direction>
          <relatedStateVariable>X_VideoPID</relatedStateVariable>
        </argument>
